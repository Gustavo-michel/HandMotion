{
    "manifest_version": 3,
    "name": "HandMotion",
    "description": "handmotion description",
    "version": "1.0",
    "permissions": [
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": ["http://localhost:5000/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/logo.png"
    }
  }
  